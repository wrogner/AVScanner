<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="customtkinter.CTk" id="avscanner" named="True">
    <property name="appearance_mode">light</property>
    <property name="geometry">640x480</property>
    <property name="minsize">500|400</property>
    <child>
      <object class="ttk.Panedwindow" id="panedwindow_1">
        <property name="orient">horizontal</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="customtkinter.CTkFrame" id="ctkframe_1" named="True">
            <property name="corner_radius">0</property>
            <property name="fg_color">#eeeeee</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="anchor">ne</property>
              <property name="expand">true</property>
              <property name="fill">both</property>
              <property name="side">left</property>
            </layout>
            <child>
              <object class="customtkinter.CTkLabel" id="ctklabel_1">
                <property name="text" translatable="yes">Sidebar</property>
                <layout manager="pack">
                  <property name="pady">10</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="customtkinter.CTkFrame" id="ctkframe_2" named="True">
            <property name="corner_radius">0</property>
            <property name="fg_color">#ffffff</property>
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">both</property>
              <property name="side">right</property>
            </layout>
            <child>
              <object class="customtkinter.CTkLabel" id="ctklabel_2">
                <property name="text" translatable="yes">Content</property>
                <layout manager="pack">
                  <property name="pady">10</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
